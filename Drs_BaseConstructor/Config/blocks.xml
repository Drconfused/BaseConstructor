<!-- ~~~~~~~~~~~~~~~~~~~~~~~~Wood Bars Series~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Vanilla Wood Bars changes -->
	<set xpath="/blocks/block[@name='woodBarsCentered']/property[@name='Texture']/@value">379</set>
